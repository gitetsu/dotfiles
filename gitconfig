[alias]
	st = status
	ss = status -sb
	ad = add
	adp = add -p
	ci = commit
	cm = commit -m
	cam = commit --amend
	chr = cherry -v
	chp = cherry-pick
	br = branch
	ba = branch -a
	co = checkout
	df = diff
	dc = diff --cached
	dw = diff --word-diff
	up = pull --rebase
	rb = rebase
	rbm = rebase master
	ls = log --stat
	lp = log -p
	l1 = log --oneline
	sb = show-branch master HEAD
	sl = stash list
	sp = stash pop
[color]
	ui = auto
[core]
	excludesfile = ~/.gitexclude
	editor = vim
	quotepath = false
	precomposeunicode = true
[push]
	default = current
